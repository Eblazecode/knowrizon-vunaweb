{
  "version": 2,
  "builds": [
    {
      "src": "knowrizon/asgi.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "apt-get update && apt-get install -y postgresql-client && pip install -r requirements.txt",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "knowrizon/asgi.py"
    }
  ]
}
